algoritmo "Enquanto Número"
// Função :
// Autor : Carla Marcondes
// Data : 17/01/2024
// Seção de Declarações 
var

   numero: inteiro

inicio

      numero <- 1
      
      Enquanto numero < 10 faca
      
               Escreval (numero)
               Numero  <- numero + 1
      
      FimEnquanto


fimalgoritmo

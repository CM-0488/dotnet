algoritmo "Repita até número"
// Função :
// Autor : Carla Marcondes
// Data : 23/01/2024
// Seção de Declarações 
var

   numero : inteiro

inicio

      numero <- 0

      repita
      
            numero <- numero +1
            
                   escreval (numero)
      
      ate numero >= 100

fimalgoritmo
